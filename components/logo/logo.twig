{% apply spaceless %}
  {% set classes = [ 'w-12', 'h-12', 'm-2', 'fill-white', 'dark:fill-black' ] %}
  {% if utilities is defined %}
    {% set classes = castle_merge_utility_classes(utilities, classes) %}
  {% endif %}
  {% set svg = '@castle_components/components/logo/icons/logo.svg' %}
  <div {{ attributes.addClass(classes).setAttribute('aria-label', 'Smithsonian Institution Logo')}}>{% include svg %}</div>
{% endapply %}
